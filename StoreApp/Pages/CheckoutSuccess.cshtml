@page

<div class="text-center">
    <h6 class="display-6">Thanks</h6>
    <p>For placing order #@OrderId</p>
    <p>Well ship you goods as soon as possible.</p>
    <a class="btn btn-primary" asp-controller="Home" asp-action="Index">Return to Home</a>
